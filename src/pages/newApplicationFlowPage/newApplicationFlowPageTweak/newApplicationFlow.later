.parent-newApplicationFlow {
  height: 100%;
  display: grid;
  grid-template-columns: 1fr 4fr;
  grid-template-rows: 1fr;
  grid-column-gap: 0px;
  grid-row-gap: 0px;
}

.wrapper-left-newApplicationFlow {
  background-color: rgb(65, 73, 89);
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  padding: 30px;
}

.wrapper-right-newApplicationFlow {
  background-color: rgb(238, 240, 242);
}

.parent-stepsWrapper-newApplicationFlow {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  grid-template-rows: repeat(3, 1fr);
  grid-column-gap: 25px;
  grid-row-gap: 25px;
  border: 1px solid black;
  border-radius: 99999px;
}

.firstBackground-stepWrapper-newApplicationFlow {
  border: 1px solid black;
  border-radius: 99999px;
  padding: 50px;
}

.rightSide-steps-newApplicationFlow {
  padding-top: 110px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.breakLine-steps-newApplicationFlow {
  padding: 5px;
  border-bottom: 0px;
  margin-top: 5px;
  border-left: 5px dotted rgb(124, 174, 161);
  height: 100px;
  opacity: 1;
}

.newGoBackIcon-newApplicationFlow {
  margin-bottom: 10px;
  color: rgb(226, 229, 232);
}

.header-newApplicationFlow {
  margin-bottom: 50px;
  font-weight: 1000;
  color: rgb(226, 229, 232);
  font-family: 'Montserrat', sans-serif;
  font-family: 'Open Sans', sans-serif;
}
.levelsLabel-newApplicationFlow {
  margin-bottom: 30px;
  color: rgb(226, 229, 232);
  font-family: 'Montserrat', sans-serif;
  font-family: 'Open Sans', sans-serif;
}

.buttonLabel-newApplicationFlow {
  font-family: 'Montserrat', sans-serif;
  font-family: 'Open Sans', sans-serif;
  font-weight: 1000;
}

.buttonWrapper-newApplicationFlow {
  margin-top: 110px;
}

.iconWrapper-newApplicationFlow {
  background-color: rgb(255, 255, 255);
  border-radius: 10px;
  width: 55px;
  height: 55px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 40px;
  border-radius: 40px;
}

.iconWrapper-newApplicationFlow:hover {
  background-color: rgb(255, 255, 255);
  width: 55px;
  height: 55px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 40px;
