.wrapperIconLabel-background-newApplicationFlow {
  background-color: rgb(86, 94, 110);
  border-radius: 40px;
}

.iconWrapper3-newApplicationFlow {
  background-color: rgb(255, 255, 255);
  border-radius: 10px;
  width: 55px;
  height: 55px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 40px;
  border-radius: 40px;
}

.iconWrapper3-newApplicationFlow::after {
  content: '';
  white-space: nowrap;
  display: block;
  font-size: 20px;
  font-weight: 1000;
}

.iconWrapper3-newApplicationFlow:hover {
  background-color: transparent;
  width: 55px;
  height: 55px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 40px;
  transform: translate(620px, 265px) scale(0.55);
  transition: 1.5s;
}

.iconWrapper3-newApplicationFlow:hover::after {
  content: 'Email Verification';
  font-family: 'Montserrat', sans-serif;
  font-family: 'Open Sans', sans-serif;
  transition: 1.5s;
}

